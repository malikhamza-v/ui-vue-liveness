{
  "name": "ui-vue-liveness",
  "version": "1.0.0",
  "description": "Vue 3 Face Liveness Detection component with AWS integration",
  "keywords": ["vue", "vue3", "face-detection", "liveness", "aws", "ui", "component"],
  "author": "Malik Hamza <malik@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/malikhamzav/ui-vue-liveness.git"
  },
  "bugs": {
    "url": "https://github.com/malikhamzav/ui-vue-liveness/issues"
  },
  "homepage": "https://github.com/malikhamzav/ui-vue-liveness#readme",
  "type": "module",
  "main": "./dist/ui-vue-liveness.umd.cjs",
  "module": "./dist/ui-vue-liveness.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/ui-vue-liveness.js",
      "require": "./dist/ui-vue-liveness.umd.cjs"
    },
    "./styles.css": "./dist/ui-vue-liveness.css"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build-lib": "run-s build-lib-only build-types",
    "preview": "vite preview",
    "build-only": "vite build",
    "build-lib-only": "vite build --mode lib",
    "build-types": "vue-tsc --build --emitDeclarationOnly",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "prepublishOnly": "npm run build-lib"
  },
  "dependencies": {
    "@aws-amplify/ui": "6.10.3",
    "@aws-sdk/client-rekognitionstreaming": "3.828.0",
    "@aws-sdk/util-format-url": "3.609.0",
    "@smithy/eventstream-serde-browser": "^4.0.4",
    "@smithy/fetch-http-handler": "^5.0.4",
    "@smithy/protocol-http": "^3.0.3",
    "@smithy/signature-v4": "5.1.2",
    "@smithy/types": "^4.3.1",
    "@mediapipe/face_detection": "~0.4.0",
    "@tensorflow-models/face-detection": "1.0.2",
    "@tensorflow/tfjs-backend-cpu": "4.11.0",
    "@tensorflow/tfjs-backend-wasm": "4.11.0",
    "@tensorflow/tfjs-backend-webgl": "4.11.0",
    "@tensorflow/tfjs-converter": "4.11.0",
    "@tensorflow/tfjs-core": "4.11.0",
    "@xstate/vue": "2.0.0",
    "tslib": "^2.5.2",
    "uuid": "^11.1.0",
    "xstate": "^4.33.6"
  },
  "peerDependencies": {
    "@aws-amplify/ui-vue": "^4.3.3",
    "vue": "^3.5.0"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.16.5",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.31.0",
    "eslint-plugin-vue": "~10.3.0",
    "jiti": "^2.4.2",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.6.2",
    "typescript": "~5.8.0",
    "vite": "^7.0.6",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vue-tsc": "^3.0.4"
  }
}
